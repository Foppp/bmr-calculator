{
  "name": "bmr-calculator",
  "version": "1.0.0",
  "description": "Calculate your Basal Metabolic Rate",
  "engines": {
    "npm": "8.x",
    "node": "17.x"
  },
  "scripts": {
    "build-client": "NODE_ENV=production webpack --mode=production --config webpack.client.js",
    "build-server": "NODE_ENV=production webpack --mode=production --config webpack.server.js",
    "prebuild": "rm -rf dist",
    "start": "node dist/server.js",
    "build": "npm run build-client && npm run build-server",
    "nodemon": "nodemon dist/server.js",
    "watch:frontEnd": "webpack -w --mode=development --config webpack.client.js",
    "watch:backEnd": "webpack -w --mode=development --config webpack.server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/register": "^7.17.7",
    "@emotion/css": "^11.7.1",
    "@emotion/react": "^11.8.2",
    "@popperjs/core": "^2.11.4",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "fastify": "^3.27.4",
    "fastify-static": "^4.6.1",
    "ignore-styles": "^5.0.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@emotion/babel-plugin": "^11.7.2",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.1",
    "sass": "^1.49.11",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-node-externals": "^3.0.0"
  }
}
